{
   "applications": {
      "gecko": {
         "strict_min_version": "48.0"
      }
   },
   "background": {
      "persistent": false,
      "scripts": [ "app/client.js", "app/eventPage.js" ]
   },
   "browser_action": {
      "default_icon": "media/logo/logo.png",
      "default_popup": "app/popup.html"
   },
   "description": "Log the current tab and your browser activity with ActivityWatch.",
   "icons": {
      "128": "media/logo/logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ActivityWatch Web Watcher",
   "permissions": [ "tabs", "alarms", "notifications", "activeTab", "storage", "http://127.0.0.1:5600/api/*", "http://127.0.0.1:5666/api/*" ],
   "short_name": "ActivityWatch",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.3"
}
